<h1>Listado de tipodocs</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipodocs.each do |tipodoc| %>
      <tr>
        <td><%= tipodoc.nombre %></td>
        <td><%= link_to 'Ver', tipodoc %></td>
        <td><%= link_to 'Editar', edit_tipodoc_path(tipodoc) %></td>
        <td><%= link_to 'Eliminar', tipodoc, method: :delete, data: { confirm: 'EstÃ¡ seguro que desea eliminar este registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipodoc', new_tipodoc_path %>
