<h1>Lista de los tipos de salidas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_salidas.each do |tipo_salida| %>
      <tr>
        <td><%= tipo_salida.nombre %></td>
        <td><%= link_to 'Ver', tipo_salida %></td>
        <td><%= link_to 'Editar', edit_tipo_salida_path(tipo_salida) %></td>
        <td><%= link_to 'Eliminar', tipo_salida, method: :delete, data: { confirm: 'Esta seguro de eliminar?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo salida', new_tipo_salida_path %>
